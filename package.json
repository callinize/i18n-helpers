{
  "name": "i18n-helpers",
  "version": "0.1.0",
  "description": "Helper to search for i18n occurences",
  "main": "index.js",
  "bin": {
    "i18n-helper": "lib/main.js"
  },
  "scripts": {
    "build": "rm -rf ./lib && babel src -d lib --ignore *.test.js",
    "coverage": "jest --coverage",
    "codacy": "npm run coverage && cat ./coverage/lcov.info | codacy-coverage && rm -rf ./coverage",
    "lint": "eslint --fix src",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "i18n",
    "translation",
    "helper"
  ],
  "author": "Bartek Polanczyk <bartosz@tenfold.com>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.5.0",
    "chalk": "^2.0.1",
    "cli-table": "^0.3.1",
    "command-line-commands": "^2.0.1",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "table": "^4.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "codacy-coverage": "^2.0.2",
    "eslint": "^4.2.0",
    "eslint-config-google": "^0.9.1",
    "jest": "^20.0.4"
  }
}
